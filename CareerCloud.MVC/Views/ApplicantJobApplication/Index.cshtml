@model IEnumerable<CareerCloud.MVC.Models.ApplicantAppliedJobs>
@{
	ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.JobName)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.JobDescription)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.CompanyName)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.City)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.State)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Website)
		</th>
		<th></th>
	</tr>

	@foreach (var item in Model)
	{
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.JobName)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.JobDescription)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.CompanyName)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.City)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.State)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Website)
			</td>
			<td>
					@Html.ActionLink("Withdraw", "Withdraw", new { id = item.AplliedJobID })
			</td>
		</tr>
	}
</table>


